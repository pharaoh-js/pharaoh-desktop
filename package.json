{
  "name": "pharaoh-nwjs",
  "version": "0.1.0",
  "description": "The front-end for a live code broadcasting app.",
  "main": "http://127.0.0.1:9090",
  "scripts": {
    "start": "npm run dev & npm run app",
    "dev": "./node_modules/.bin/webpack-dev-server --quiet --port 9090 --hot --inline",
    "app": "./node_modules/.bin/nwjs",
    "lint": "node node_modules/.bin/eslint -c .eslintrc src/**"
  },
  "window": {
    "title": "Pharaoh",
    "icon": "icon.png",
    "toolbar": true,
    "frame": false,
    "single-instance": false,
    "width": 1600,
    "height": 900,
    "min_width": 400,
    "min_height": 300,
    "position": "center",
    "dom_storage_quota": 64,
    "as_desktop": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pharaoh-js/pharaoh.git"
  },
  "keywords": [
    "live",
    "student",
    "instructor",
    "ide",
    "editor",
    "atom",
    "react"
  ],
  "author": "Pharaoh JS Team",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/pharaoh-js/pharaoh-desktop/issues"
  },
  "homepage": "https://github.com/pharaoh-js/pharaoh-desktop#readme",
  "dependencies": {
    "codemirror": "^5.10.0",
    "firebase": "^2.3.2",
    "firepad": "^1.3.0",
    "history": "^1.17.0",
    "npm-start": "^1.4.2",
    "nwjs": "0.0.15",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-fontawesome": "^0.3.3",
    "react-hot-loader": "^1.3.0",
    "react-inline-css": "^2.0.1",
    "react-router": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.14.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
